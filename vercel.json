{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python@3.9.0"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/api/predict",
      "dest": "/api/predict.py"
    },
    {
      "src": "/api/data-analysis",
      "dest": "/api/data_analysis.py"
    },
    {
      "src": "/api/spiritual",
      "dest": "/api/spiritual_perturbation.py"
    },
    {
      "src": "/api/generate-tweet",
      "dest": "/api/generate_tweet.py"
    },
    {
      "src": "/api/train-model",
      "dest": "/api/model_train.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}