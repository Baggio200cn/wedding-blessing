def generate_markdown(title, analysis_table, analysis_chart_path, prediction_results):
    """
    生成中文Markdown文件
    :param title: 标题
    :param analysis_table: 数据分析表格
    :param analysis_chart_path: 分析图路径
    :param prediction_results: 预测结果
    :return: Markdown字符串
    """
    markdown = f"""
## {title}

### 数据分析过程
以下是数据分析表格：
{analysis_table}

分析图表如下：
![分析图表]({analysis_chart_path})

### 预测结果展示
前区预测号码：{prediction_results['front']}
后区预测号码：{prediction_results['back']}
"""
    return markdown

if __name__ == "__main__":
    # 示例调用
    prediction_results = {"front": [1, 12, 23, 34, 45], "back": [6, 7]}
    analysis_table = "<table><tr><td>示例表格</td></tr></table>"
    analysis_chart_path = "charts/analysis_chart.png"
    markdown_content = generate_markdown("LSTM 模块分析", analysis_table, analysis_chart_path, prediction_results)
    with open("module_lstm.md", "w", encoding="utf-8") as f:
        f.write(markdown_content)
    print("Markdown 文件已生成")
